{
  "Scheme": "semantic",
  "VersionFile": "./VERSION", 
  "Files": [
    {
      "path":  "./VERSION"
      "line": 1
    }
  ],
  "rules": [
    {
      "trigger": "commit",
      "branch": "hotfix",
      "bump": "build"
    },
    {
      "trigger": "commit",
      "bump": "minor",
      "branch": "master",
      "reset": "build"
    },
    {
      "trigger": "commit",
      "bump": "major",
      "branch": "release",
      "reset": [
        "minor",
        "build"
      ]
    }
  ]}
